<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.project.doctor_fish_back.repository.MonthMapper">

    <select id="monthList" resultType="com.project.doctor_fish_back.entity.Month">
        select
            id,
            month
        from
            month_tb
        order by id
    </select>

</mapper>